{
  "name": "Redcurry asset management",
  "description": "Enables safe, multi signatory on-chain supply and its underlying reserve asset control.",
  "version": "1.2.1",
  "main": "dist/server.js",
  "license": "UNLICENSED, all rights reserved",
  "author": "Kaspar Triebstok, Redcurry",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "test": "mocha -r ts-node/register 'test/index.spec.ts'",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "serve-staging": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=staging node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@fireblocks/fireblocks-web3-provider": "^1.2.1",
    "@google-cloud/secret-manager": "^4.2.1",
    "alchemy-sdk": "^2.8.0",
    "axios": "^1.2.0",
    "body-parser": "^1.20.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "fireblocks-sdk": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "moralis": "^2.18.2",
    "nodemailer": "^6.8.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "nodemailer-sendgrid": "^1.0.3",
    "passport-2fa-totp": "^0.0.1",
    "passport-google-oauth20": "^2.0.0",
    "payload": "^1.6.4",
    "payload-plugin-google-one-tap": "^1.0.7",
    "random-words": "^1.2.0",
    "react-toastify": "^9.1.2",
    "uuid": "^9.0.0",
    "ws": "^8.11.0",
    "xrpl": "^2.11.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.9",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.4",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.6",
    "sinon": "^15.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5",
    "web3": "^1.9.0"
  }
}
